<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
    
<!--    注册servlet-->
    <servlet>
        <servlet-name>servlet_sigh_up</servlet-name>
        <servlet-class>Controller.UserController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>servlet_sigh_up</servlet-name>
        <url-pattern>/servlet_sigh_up</url-pattern>
    </servlet-mapping>


    <servlet>
        <servlet-name>OrderHotel</servlet-name>
        <servlet-class>Controller.OrderHotelController</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>OrderHotel</servlet-name>
        <url-pattern>/OrderHotel</url-pattern>
    </servlet-mapping>

</web-app>